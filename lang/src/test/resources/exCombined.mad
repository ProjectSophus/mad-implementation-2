
Sealed Concept Rings;

Template Ring [R is Concept] {
    Example Rings <R>;
    
    Operation on <R> {Addition, Multiplication};
    Function on <R> Inversion
    
    Machine IntegerInclusion :: Integers -> <R>
};

Concept Polynomials[C :: Rings];

Extend Template Ring [R is Concept] {
    UseTemplate Ring [Polynomials[<R>]];
};

Concept Integers;
UseTemplate Ring [Integers];

